ull;\n  y = null;\n  width = 100;\n  speed = 15;\n  active = false;\n  direction = DIRECTION.Right;\n  ctx = null;\n  camera = null;\n\n  constructor (ctx, camera) {\n    this.ctx = ctx;\n    this.camera = camera;\n  }\n\n  draw = () => {\n    this.ctx.strokeStyle = COLOURS.BULLET;\n    this.ctx.lineWidth = this.lineWidth;\n    this.ctx.beginPath();\n    this.ctx.moveTo(this.x -this.camera.x, this.y);\n    this.ctx.lineTo(this.x + this.width -this.camera.x, this.y);\n    this.ctx.stroke();\n  };\n\n  shoot = (x, y, direction) => {\n    this.active = true;\n    this.x = x;\n    this.y = y;\n    this.direction = direction;\n  }\n\n  move = () => {\n    if(this.active) {\n        if(this.direction === DIRECTION.Right) {\n            this.x = this.x + this.speed;\n        } else {\n            this.x = this.x - this.speed;\n        }\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"c2fb990833b33b00e400c8389b24b3c2","cacheData":{"env":{}}}